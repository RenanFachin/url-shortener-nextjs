generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model URL {
  id          String   @id @default(cuid())
  originalUrl String
  shorCode    String   @unique
  createAt    DateTime @default(now())
  visits      Int      @default(0)
}
